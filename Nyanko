gg.searchNumber("32400", 4)
R = gg.getResults(1)
local v = {}
v[1] = {}
v[1].address = R[1].address -0x9228
v[1].flags = 4
v[1].value = 256
gg.setValues(v)
local v = {}
v[1] = {}
v[1].address = R[1].address -0x9228
v[1].flags = 4
v[1].value = 256
gg.setValues(v)
gg.toast(" ğŸ›Œ ã‚­ãƒ£ãƒ©2æ®µè¡¨ç¤º : âœ… ğ‘ºğ’–ğ’„ğ’„ğ’†ğ’”ğ’” ")
end

function qBACK()
qMAINMENU() end

function qBACK1()
qSCRIPT1() end

function qBACK2()
qNKHS() end

function qBACK4()
qMT() end

function qEND()
gg.clearResults()
gg.setVisible(true)
print("ã€€\nâ”â”â”â”â”â”â”â”â”â”â”â”â”“\nâ”ƒã€€ã€€ã€€ãƒã‚¤ãƒã‚¤ğŸ‘‹ã€€ã€€ã€€â”ƒ\nâ”ƒã€€ã€€åˆ¶ä½œè€…ï¸°Araã€€ã€€â”ƒ\nâ”—â”â”â”â”â”â”â”â”â”â”â”â”›\n")
os.exit()
